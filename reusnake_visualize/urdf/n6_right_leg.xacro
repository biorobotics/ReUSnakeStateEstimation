<?xml version="1.0"?>

<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:property name="PI" value="3.1415926535897931" />
  <!-- NOTE: joint must match the name of the module -->
  <xacro:macro name="right_leg" params="leg mod1 mod2 mod3">

    <link
    name="${leg}_Link0">
    <inertial>
      <origin
        xyz="-0.013312 -0.0013264 -0.0010606"
        rpy="0 0 0" />
      <mass
        value="0.5" />
      <inertia
        ixx="0"
        ixy="0"
        ixz="0"
        iyy="0"
        iyz="0"
        izz="0" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://titan6_general_cpp/meshes/N6_Link0.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://titan6_general_cpp/meshes/N6_Link0.STL" />
      </geometry>
    </collision>
  </link>


  <link
    name="${leg}_Link1">
    <inertial>
      <origin
        xyz="0 0 0.048171"
        rpy="1.5708 0 0" />
      <mass
        value="1.367" />
      <inertia
        ixx=".002"
        ixy="0."
        ixz="0."
        iyy=".002"
        iyz="0."
        izz=".002" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="1.5708 0 0" />
      <geometry>
        <mesh
          filename="package://titan6_general_cpp/meshes/N6_Link1_2.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="1.5708 0 0" />
      <geometry>
        <mesh
          filename="package://titan6_general_cpp/meshes/N6_Link1_2.STL" />
      </geometry>
    </collision>
  </link>

  <joint
    name="${mod1}"
    type="revolute">
    <origin
      xyz="0 0 0.02205"
      rpy="0 0 -1.5708" />
    <parent
      link="${leg}_Link0" />
    <child
      link="${leg}_Link1" />
    <axis
      xyz="0 0 1" />
    <dynamics damping=".1" friction="0.2"/>
    <limit effort="5000.0" lower="-${PI/2}" upper="${PI/2}" velocity="2.208932"/>
  </joint>
	<!-- Add gazebo plugin joint force sensor -->
	<gazebo reference="${mod1}">
		<provideFeedback>true</provideFeedback>
	</gazebo>
	<gazebo>
	      <plugin filename="libgazebo_ros_ft_sensor.so" name="ft_sensor_plugin">
		<topicName>titan6/jointFeedback/${leg}/ftbetween01/${mod1}</topicName>
		<jointName>${mod1}</jointName>
		<updateRate>10.0</updateRate>
	      </plugin>
	</gazebo>

  <transmission name="${mod1}_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="${mod1}">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="${mod1}_motor">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>300</mechanicalReduction>
    </actuator>
  </transmission>


  <link
    name="${leg}_Link2">
    <inertial>
      <origin
        xyz="0.00043046 0.6759 0.056602"
        rpy="0 0 0" />
      <mass
        value="0.9687" />
      <inertia
        ixx="0.1249"
        ixy="0"
        ixz="0"
        iyy="0"
        iyz="0"
        izz="0.1250" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 -1.5708 -3.1416" />
      <geometry>
        <mesh
          filename="package://titan6_general_cpp/meshes/N6_Link1_3.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.25098 0.25098 0.25098 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 -1.5708 -3.1416" />
      <geometry>
        <mesh
          filename="package://titan6_general_cpp/meshes/N6_Link1_3.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="${mod2}"
    type="revolute">
    <origin
      xyz="0 0 0.0514"
      rpy="0 -1.5708 0" />
    <parent
      link="${leg}_Link1" />
    <child
      link="${leg}_Link2" />
    <axis
      xyz="0 0 -1" />
    <dynamics damping=".1" friction="0.2"/>
    <limit effort="5000.0" lower="-${PI/6}" upper="${PI/2}" velocity="2.208932"/>
  </joint>
	<!-- Add gazebo plugin joint force sensor -->
	<gazebo reference="${mod2}">
		<provideFeedback>true</provideFeedback>
	</gazebo>
	<gazebo>
	      <plugin filename="libgazebo_ros_ft_sensor.so" name="ft_sensor_plugin">
		<topicName>titan6/jointFeedback/${leg}/ftbetween12/${mod2}</topicName>
		<jointName>${mod2}</jointName>
		<updateRate>10.0</updateRate>
	      </plugin>
	</gazebo>

  <transmission name="${mod2}_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="${mod2}">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="${mod2}_motor">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>300</mechanicalReduction>
    </actuator>
  </transmission>

  <link
    name="${leg}_Link3">
    <inertial>
      <origin
        xyz="3.6557E-09 -0.44069 0.041072"
        rpy="0 0 0" />
      <mass
        value="0.3727" />
      <inertia
        ixx="0.0379"
        ixy="0"
        ixz="0"
        iyy="0"
        iyz="0"
        izz="0.0379" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="1.5708 0 ${PI}" />
      <geometry>
        <mesh
          filename="package://titan6_general_cpp/meshes/N6_Link1_4.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.89804 0.91765 0.92941 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="1.5708 0 ${PI}" />
      <geometry>
        <mesh                                                                                                                                                                                 
          filename="package://titan6_general_cpp/meshes/N6_Link1_4.STL" />
      </geometry>
       <surface>
        <friction>
          <ode>
            <mu>0.9</mu>
            <mu2>0.9</mu2>
          </ode>
        </friction>
      </surface> 
    </collision>
  </link>
  <joint
    name="${mod3}"
    type="revolute">
    <origin
      xyz="0 1.0255 0.08505"
      rpy="0 0 1.5708" />
    <parent
      link="${leg}_Link2" />
    <child
      link="${leg}_Link3" />
    <axis
      xyz="0 0 1" />
    <dynamics damping=".1" friction="0.2"/>
    <limit effort="5000.0" lower="-${PI}" upper="${PI/4}" velocity="2.208932"/>
  </joint>
	<!-- Add gazebo plugin joint force sensor -->
	<gazebo reference="${mod3}">
		<provideFeedback>true</provideFeedback>
	</gazebo>
	<gazebo>
	      <plugin filename="libgazebo_ros_ft_sensor.so" name="ft_sensor_plugin">
		<topicName>titan6/jointFeedback/${leg}/ftbetween23/${mod3}</topicName>
		<jointName>${mod3}</jointName>
		<updateRate>10.0</updateRate>
	      </plugin>
	</gazebo>


  <transmission name="${mod3}_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="${mod3}">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="${mod3}_motor">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>300</mechanicalReduction>
    </actuator>
  </transmission>

  <link
    name="${leg}_Foot">
    <fdir1>1 0 0</fdir1>
    <maxContacts>1</maxContacts>
    <minDepth>0.005</minDepth>
    <gravity>true</gravity>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <sphere radius=".015"/>
      </geometry>
      <mu1>0.9</mu1>
      <mu2>0.9</mu2>
      <static>false</static>
      <selfCollide>true</selfCollide>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <sphere radius=".015"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0.9</mu>
            <mu2>0.9</mu2>
          </ode>
        </friction>
      </surface>
    </collision>
  </link>
  <joint
    name="${leg}_foot_joint"
    type="fixed">
    <origin
      xyz="1.01 0 0"
      rpy="0 0 0" />
    <parent
      link="${leg}_Link3" />
    <child
      link="${leg}_Foot" />
  </joint> 

  </xacro:macro>
</robot>
