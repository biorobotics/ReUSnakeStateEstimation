<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">  
  <xacro:macro name="m6_left_leg" params="prefix mod1 mod2 mod3 base_x base_y base_ang">
    <link
      name="${mod1}__INPUT_INTERFACE">
      <inertial>
        <origin
          xyz="0.015005 0.001775 0.015011"
          rpy="0 0 0" />
        <mass
          value="0.9023" />
        <inertia
          ixx="0"
          ixy="0"
          ixz="0"
          iyy="0"
          iyz="0"
          izz="0" />
      </inertial>
      <visual>
        <origin
          xyz="0 0 0.015"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://titan6_general_cpp/meshes/M6_base_motor.STL" />
        </geometry>
        <material
          name="">
          <color
            rgba="0.79216 0.81961 0.93333 1" />
        </material>
      </visual>
      <collision>
        <origin
          xyz="0 0 0.015"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://titan6_general_cpp/meshes/M6_base_motor.STL" />
        </geometry>
      </collision>
    </link>

    <joint
      name="leg${prefix}__OUTPUT_INTERFACE"
      type="fixed">
      <origin
        xyz="${base_x} ${base_y} 0"
        rpy="0 0 ${pi+base_ang}" />
      <parent
        link="base_link" />
      <child
        link="${mod1}__INPUT_INTERFACE" />
      <axis
        xyz="0 0 1" />
      <limit
        effort="0"
        velocity="0" />
    </joint>

    <link
      name="${mod2}__INPUT_INTERFACE">
      <inertial>
        <origin
          xyz="-0.012554 -0.0090693 0.0503697"
          rpy="0 0 0" />
        <mass
          value="1.0345" />
        <inertia
          ixx="0.002"
          ixy="0"
          ixz="0"
          iyy="0.002"
          iyz="0"
          izz="0.002" />
      </inertial>
      <visual>
        <origin
          xyz="0 0 0"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://titan6_general_cpp/meshes/M6_left_link1_red.STL" />
        </geometry>
        <material
          name="">
          <color
            rgba="0.75294 0.75294 0.75294 1" />
        </material>
      </visual>
      <collision>
        <origin
          xyz="0 0 0"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://titan6_general_cpp/meshes/M6_left_link1_red.STL" />
        </geometry>
      </collision>
    </link>

    <joint
      name="${mod1}"
      type="revolute">
      <origin
        xyz="0 0 0.03105"
        rpy="0 0 ${pi}" />
      <parent
        link="${mod1}__INPUT_INTERFACE" />
      <child
        link="${mod2}__INPUT_INTERFACE" />
      <axis
        xyz="0 0 1" />
      <dynamics 
        damping=".1" 
        friction="0.2"/>
      <limit 
        effort="20.0" 
        lower="${-pi/2}" 
        upper="${pi/2}" 
        velocity="3.14159265"/>
    </joint>

    <transmission name="${mod1}_trans">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="${mod1}">
        <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      </joint>
      <actuator name="${mod1}_motor">
        <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        <mechanicalReduction>300</mechanicalReduction>
      </actuator>
    </transmission>


    <link
      name="${mod3}__INPUT_INTERFACE">
      <inertial>
        <origin
          xyz="0.27885723 0.00143862 -0.00919708"
          rpy="0 0 0" />
        <mass
          value="1.1204" />
        <inertia
          ixx="0.125"
          ixy="0"
          ixz="0"
          iyy="0"
          iyz="0"
          izz="0.125" />
      </inertial>
      <visual>
        <origin
          xyz="0 0 0"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://titan6_general_cpp/meshes/M6_link2_red.STL" />
        </geometry>
        <material
          name="">
          <color
            rgba="0.75294 0.75294 0.75294 1" />
        </material>
      </visual>
      <collision>
        <origin
          xyz="0 0 0"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://titan6_general_cpp/meshes/M6_link2_red.STL" />
        </geometry>
      </collision>
    </link>

    <joint
      name="${mod2}"
      type="revolute">
      <origin
        xyz="0 0.00855 0.055"
        rpy="${-pi/2} 0 0" />
      <parent
        link="${mod2}__INPUT_INTERFACE" />
      <child
        link="${mod3}__INPUT_INTERFACE" />
      <axis
        xyz="0 0 1" />
      <dynamics 
        damping=".1" 
        friction="0.2"/>
      <limit 
      effort="38.0" 
      lower="${-pi/2}" 
      upper="${pi/2}" 
      velocity="1.57079632"/>
    </joint>

    <transmission name="${mod2}_trans">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="${mod2}">
        <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      </joint>
      <actuator name="${mod2}_motor">
        <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        <mechanicalReduction>300</mechanicalReduction>
      </actuator>
    </transmission>


    
    <link
      name="leg${prefix}__LAST_LINK">
      <inertial>
        <origin
          xyz="0.12121923 0 0.01723596"
          rpy="0 0 0" />
        <mass
          value="0.1617" />
        <inertia
          ixx="0.0379"
          ixy="0"
          ixz="0"
          iyy="0"
          iyz="0"
          izz="0.0379" />
      </inertial>
      <visual>
        <origin
          xyz="0 0 0"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://titan6_general_cpp/meshes/M6_link3_red.STL" />
        </geometry>
        <material
          name="">
          <color
            rgba="0.75294 0.75294 0.75294 1" />
        </material>
      </visual>
      <collision>
        <origin
          xyz="0 0 0"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://titan6_general_cpp/meshes/M6_link3_red.STL" />
        </geometry>
      </collision>
    </link>

    <joint
      name="${mod3}"
      type="revolute">
      <origin
        xyz="0.3195 0 -0.03105"
        rpy="3.1415 0 0" />
      <parent
        link="${mod3}__INPUT_INTERFACE" />
      <child
        link="leg${prefix}__LAST_LINK" />
      <axis
        xyz="0 0 1" />
      <dynamics 
        damping=".1" 
        friction="0.2"/>
      <limit 
        effort="20.0" 
        lower="${-pi}" 
        upper="${pi}" 
        velocity="3.14159265"/>
    </joint>
    
    <transmission name="${mod3}_trans">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="${mod3}">
        <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      </joint>
      <actuator name="${mod3}_motor">
        <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        <mechanicalReduction>300</mechanicalReduction>
      </actuator>
    </transmission>

  </xacro:macro>
</robot>